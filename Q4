x = int(input('Enter 3 no.s\n'))
y = int(input(''))
z = int(input(''))
if x >= y:
    if y >= z:
        print(f'Greatest of 3 no.s is {str(x)}')
    elif x >= z:
        print(f'Greatest of 3 no.s is {str(x)}')
    else:
        print(f'Greatest of 3 no.s is {str(z)}')
elif x >= z:
    print(f'Greatest of 3 no.s is {str(y)}')
elif y >= z:
    print(f'Greatest of 3 no.s is {str(y)}')
else:
    print(f'Greatest of 3 no.s is {str(z)}')
